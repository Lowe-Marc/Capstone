    <div class="content-wrapper">
        <div class="container-fluid">
            <!-- Bootstrap core JavaScript-->
            <script src="~/vendor/jquery/jquery.min.js"></script>
            <script src="~/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
            <!-- Core plugin JavaScript-->
            <script src="~/vendor/jquery-easing/jquery.easing.min.js"></script>
            <!-- Page level plugin JavaScript-->
            <script src="~/vendor/chart.js/Chart.min.js"></script>
            <script src="~/vendor/datatables/jquery.dataTables.js"></script>
            <script src="~/vendor/datatables/dataTables.bootstrap4.js"></script>
            <!-- Custom scripts for all pages-->
            <script src="~/js/sb-admin.min.js"></script>
            <!-- Custom scripts for this page-->
            <script src="~/js/sb-admin-datatables.min.js"></script>
            @* TODO: This is raising an error in the console *@
            @*<script src="~/js/sb-admin-charts.min.js"></script>*@
            <!-- Cytoscape.js -->
            <script src="http://cdnjs.cloudflare.com/ajax/libs/fastclick/1.0.3/fastclick.min.js"></script>
            <script src="https://cdnjs.cloudflare.com/ajax/libs/cytoscape/3.2.10/cytoscape.min.js"></script>
            <script src="https://cdnjs.cloudflare.com/ajax/libs/qtip2/3.0.3/jquery.qtip.min.js"></script>
            <script src="https://cdn.rawgit.com/cytoscape/cytoscape.js-qtip/2.7.0/cytoscape-qtip.js"></script>
            <script src="~/js/cytoscape-cose-bilkent.js"></script>
            <!-- Custom scripts for handling cytoscape-->
            <script src="~/js/Custom/cytoscape-helpers.js"></script>
            <!-- One off UI functions-->
            <script src="~/js/Custom/ui-helpers.js"></script>
            <!-- Custom scripts for handling animations -->
            @*<script src="~/js/Custom/animation-functionality.js"></script>*@
            <script src="~/js/Custom/simulation-interface.js"></script>
            <script src="~/js/Custom/Generic/configuration-module.js"></script>
            <script src="~/js/Custom/Generic/toolbar-module.js"></script>
            <script src="~/js/Custom/Generic/animation-module.js"></script>
            <!-- General UI stuff -->
            @*<script src="~/js/Custom/ui-functionality.js"></script>*@
            <!-- Full screen functionality -->
            <script src="~/js/fullscreen/jquery.fullscreen-min.js"></script>
            <!-- Grid Snapping -->
            <script src="~/js/GridSnapping/cytoscape.js-grid-guide-2.2.1/cytoscape-grid-guide.js"></script>
            <!-- Edge Handles -->
            @*<script src="~/js/EdgeHandles/cytoscape.js-edgehandles-3.4.1/cytoscape-edgehandles.js"></script>*@


            <div class="jumbotron" style="height:3%">
                <h1 id="simulation-title"></h1>
                <p class="lead" id="simulation-description"></p>
            </div>

            <div id="simulation-area" content="width=device-width, initial-scale=1">
                <div style="display:none" id="simulation-header">
                    <div id="operation">
                        <div>
                            <select class="first" id="configuration-selector"></select>
                        </div>
                        <div class="svg-expand pointer second">
                            <span class="fa oi icon" id="fullscreen-enter" data-glyph="fullscreen-enter" title="Fullscreen" aria-hidden="true"></span>
                            <span class="fa oi icon" style="display: none" id="fullscreen-exit" data-glyph="fullscreen-exit" title="Fullscreen" aria-hidden="true"></span>
                            <span class="fa oi icon" style="display: none" id="lock-unlocked" data-glyph="lock-unlocked" title="Unlock Node Positions" aria-hidden="true"></span>
                            <span class="fa oi icon" id="lock-locked" data-glyph="lock-locked" title="Lock Node Positions" aria-hidden="true"></span>
                            <span class="fa oi icon" id="disable-grid-snapping" data-glyph="move" title="Disable Grid Snapping" aria-hidden="true"></span>
                            <span class="fa oi icon" style="display: none" id="enable-grid-snapping" data-glyph="grid-four-up" title="Enable Grid Snapping" aria-hidden="true"></span>
                            <span class="fa oi icon" id="save-config" data-glyph="cloud-upload" title="Save Configuration" aria-hidden="true"></span>
                            <span class="fa oi icon" id="simulate" data-glyph="calculator" title="Run Simulation" aria-hidden="true"></span>
                            <span class="fa oi icon disabled" id="play" data-glyph="media-play" title="Play Animation" aria-hidden="true"></span>
                            <span class="fa oi icon disabled" id="pause" data-glyph="media-pause" title="Pause Animation" aria-hidden="true"></span>
                            <span class="fa oi icon disabled" id="backward" data-glyph="media-skip-backward" title="Move Back a Frame" aria-hidden="true"></span>
                            <input id="frame-tracker" value="0" onkeypress="this.style.width = ((this.value.length + 2) * 8) + 'px';">
                            <label id="max-frame-count">/0</label>
                            <span class="fa oi icon disabled" id="forward" data-glyph="media-skip-forward" title="Move Forward a Frame" aria-hidden="true"></span>
                            <label id="add-connection-label" style="display:none">Click a node to connect to</label>
                        </div>
                    </div>
                </div>

                <div id="simulation-config" style="display:none"></div>
                <div id="simulation-display">
                    @RenderBody()
                </div>
            </div>

            <!-- Area Chart Example-->
            @*<div class="card mb-3">
            <div class="card-header">
